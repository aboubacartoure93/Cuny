<h1>Housings</h1>

<table>
  <thead>
    <tr>
      <th>Rentaltype</th>
      <th>Numberofbedrooms</th>
      <th>Numberofbathrooms</th>
      <th>Houseprice</th>
      <th>Availabledate</th>
      <th>Student</th>
      <th>Homedescription</th>
      <th>Address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @housings.each do |housing| %>
      <tr>
        <td><%= housing.rentalType %></td>
        <td><%= housing.numberofBedrooms %></td>
        <td><%= housing.numberofBathrooms %></td>
        <td><%= housing.housePrice %></td>
        <td><%= housing.availableDate %></td>
        <td><%= housing.student_id %></td>
        <td><%= housing.homeDescription %></td>
        <td><%= housing.address %></td>
        <td><%= link_to 'Show', housing %></td>
        <td><%= link_to 'Edit', edit_housing_path(housing) %></td>
        <td><%= link_to 'Destroy', housing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

