<p id="notice"><%= notice %></p>

<h1>Other Articles</h1>

<table>
  <thead>
    <tr>
      <th>Otherstype</th>
      <th>Othersprice</th>
      <th>Availablenow</th>
      <th>Student</th>
      <th>Othersdescription</th>
      <th>Otherslocation</th>
      <th>Condition</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @other_articles.each do |other_article| %>
      <tr>
        <td><%= other_article.othersType %></td>
        <td><%= other_article.othersPrice %></td>
        <td><%= other_article.availableNow %></td>
        <td><%= other_article.student_id %></td>
        <td><%= other_article.othersDescription %></td>
        <td><%= other_article.othersLocation %></td>
        <td><%= other_article.condition %></td>
        <td><%= link_to 'Show', other_article %></td>
        <td><%= link_to 'Edit', edit_other_article_path(other_article) %></td>
        <td><%= link_to 'Destroy', other_article, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

